package kr.or.ddit.board.vo;

public class WebMsgDTO {

	private int num;
	private String send_id;
	private String receive_id;
	private String msg;
	private String m_date;
	
	public WebMsgDTO(String receive_id) {
		super();
		this.receive_id = receive_id;
	}
	public WebMsgDTO(int num, String send_id, String receive_id, String msg, String m_date) {
		super();
		this.num = num;
		this.send_id = send_id;
		this.receive_id = receive_id;
		this.msg = msg;
		this.m_date = m_date;
	}
	public WebMsgDTO(String send_id, String receive_id, String msg) {
		super();
		this.send_id = send_id;
		this.receive_id = receive_id;
		this.msg = msg;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public String getSend_id() {
		return send_id;
	}
	public void setSend_id(String send_id) {
		this.send_id = send_id;
	}
	public String getReceive_id() {
		return receive_id;
	}
	public void setReceive_id(String receive_id) {
		this.receive_id = receive_id;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public String getM_date() {
		return m_date;
	}
	public void setM_date(String m_date) {
		this.m_date = m_date;
	}
}

